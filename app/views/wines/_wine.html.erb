<tr>
  <td>
  	<%= link_to wine.producer, wine %> <%= wine.name %>
  </td>
  <td>
    <%= wine.country %>
  </td>
  <td>
  	<%= wine.vintage %>
  </td>
  <td>
  	<%= wine.price %>
  </td>
<% if current_user.admin? && !current_user?(@user) %>
	<td> | <%= link_to 'delete', wine, method: :delete,
																data: { confirm: "You sure?"} %>
	</td>
<% end %>															
</tr>